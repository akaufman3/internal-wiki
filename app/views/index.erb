<section id="main" class="articleContainer">
		<% @articles.each do |article| %>
			<article class="article">
				<section class="rating">
					<h4><%= article['rating'] %></h4>
				</section>
				<section class="articleInfo">
					<a href="#">
						<h3><%= article['title'] %></h3>
					</a>
					<h6><%= article['author'] %></h6>
				</section>
				<section class="articleDate">
					<h4><%= article['date_created'] %></h4>
				</section>
			</article>
		<% end %>
	</div>
</section>